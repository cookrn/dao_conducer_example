<div class="row">
  <div class="span12">
    <h1>Dog Walkers</h1>
  </div>
</div>
<div class="row">
  <div class="span12">
    <pre><%= @dog_walkers %></pre>
  </div>
</div>
<div class="row">
  <div class="span12">
    <table class="table table-striped table-bordered">
      <thead>
        <tr>
          <th>Dog Walker</th>
          <th>Dogs</th>
          <th>Started Walking</th>
          <th>&nbsp;</th>
        </tr>
      </thead>
      <tbody>
        <% @dog_walkers.each do | dog_walker | %>
          <tr>
            <td><%= dog_walker.attributes.name %></td>
            <td><%= dog_walker.attributes.dogs.size %></td>
            <td><%= dog_walker.attributes.created_at %></td>
            <td>
              <div class="pull-right">
                <a href="<%= dog_walker_path dog_walker.id %>" class="btn btn-primary">View</a>
                <a href="<%= edit_dog_walker_path dog_walker.id %>" class="btn btn-warning">Edit</a>
              </div>
            </td>
          </tr>
        <% end %>
      </tbody>
    </table>
  </div>
</div>
